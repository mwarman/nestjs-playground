
############################################################
# Example .env file for NestJS Playground
#
# Copy this file to `.env` and adjust values as needed.
#
# This file defines environment variables used to configure
# the application at runtime. Variables can be accessed via
# process.env or configuration libraries.
############################################################

# Application Settings
# APP_PORT: The port on which the application will run.
# Change this if you want to use a different port.
APP_PORT=3001

# Logging Settings
# LOGGING_LEVEL: The logging level for the application.
# Options: fatal, error, warn, log, debug, verbose
LOGGING_LEVEL=debug